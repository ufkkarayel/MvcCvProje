@model MvcCvProje.Models.Entity.TblSertifikalarim
@{
    ViewBag.Title = "YeniSertifika";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}








<br />
<h4>Sertifika Giriş Sayfası</h4>
<br />
<form method="post">
    <br />
    @Html.Label("Sertifika açıklaması")
    @Html.TextBoxFor(x => x.Aciklama, new { @class = "form-control" })
    <br />
    @Html.Label("Tarih")
    @Html.TextBoxFor(x => x.Tarih, new { @class = "form-control" })

    <br />
    <button class="btn btn-outline-primary">Kaydet</button>

</form>